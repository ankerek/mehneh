{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "babel/generator-star-spacing": [1, "after"],
    "babel/object-shorthand": 1,
    "babel/no-await-in-loop": 1,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "eqeqeq": [1, "allow-null"],
    "no-unneeded-ternary": 1,
    "brace-style": 1,
    "space-before-blocks": 1,
    "no-unused-vars": 1
  },
  "plugins": [
    "babel",
    "react"
  ]
}
